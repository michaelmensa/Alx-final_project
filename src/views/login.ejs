<body> 
	<div class="main"> 
		<h1>Clinic Login</h1> 
		<form method="post"> 
			<label for="email">Email:</label> 
			<input type="email" id="email"
				name="email"
				placeholder="Enter your clinic email" required> 

			<label for="password">Password:</label> 
			<input type="password" id="password"
				name="password"
				placeholder="Enter your password" required> 
			<span id="pass"></span> 
			<div class="wrap"> 
				<button type="button" onclick="login()"> 
				Submit 
				</button> 
			</div>
			<p id="errorMessage" style="color: red; display: none">Clinic Not found</p>
		</form>
